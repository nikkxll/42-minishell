echo |>
echo |> a
echo |<>a
echo |>>a
echo |>>a|<a
echo a |>>a|<a
echo a&&b |>>a|<a
echo a&|&b |>>a|<a
echo a&''&b |>>a|<a
echo a&&b'' |>>a|<a

